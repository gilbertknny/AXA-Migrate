<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Service_Quality</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Service_Quality__c &lt;0,Service_Quality__c &gt;100)</errorConditionFormula>
    <errorDisplayField>Service_Quality__c</errorDisplayField>
    <errorMessage>Service Quality should be in 0 to 100.</errorMessage>
</ValidationRule>
