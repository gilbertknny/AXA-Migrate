<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>isWelcomeCallPeriod__c</fullName>
    <formula>IF(Contact__r.Campaign_Type__c = &apos;TelUW&apos;,
   (TODAY() - DATEVALUE(WelcomeCall_Schedule_Date__c)) &lt;= 29,
   (TODAY() - Contact__r.WelcomeCall_Issued_Date__c) &lt;= 
   (IF(WEEKDAY(Contact__r.WelcomeCall_Issued_Date__c ) = 2,17,19))
)</formula>
    <label>isWelcomeCallPeriod</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
