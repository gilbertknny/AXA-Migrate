<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_Uncontacted_Status</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(Call_Status_Config__c, &apos;Uncontacted&apos;),  ISNULL(Total_Uncontacted_New__c),NOT( CONTAINS(RecordType.Name, &apos;Collection AMFS&apos;) ) )</errorConditionFormula>
    <errorDisplayField>Total_Uncontacted_New__c</errorDisplayField>
    <errorMessage>This Field Cannot Be Empty</errorMessage>
</ValidationRule>
