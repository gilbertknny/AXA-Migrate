<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Wording</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Wording__c &lt;0,Wording__c &gt;100)</errorConditionFormula>
    <errorDisplayField>Wording__c</errorDisplayField>
    <errorMessage>Wording should be in 0 to 100.</errorMessage>
</ValidationRule>
