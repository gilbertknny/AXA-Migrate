<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Greeting_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Greeting__c&lt;0,Greeting__c&gt;100)</errorConditionFormula>
    <errorDisplayField>Greeting__c</errorDisplayField>
    <errorMessage>Greeting score should be in 0 to 100.</errorMessage>
</ValidationRule>
