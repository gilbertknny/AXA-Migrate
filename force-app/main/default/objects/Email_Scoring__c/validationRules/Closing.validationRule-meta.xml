<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Closing</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Closing__c &lt;0,Closing__c &gt;100)</errorConditionFormula>
    <errorDisplayField>Closing__c</errorDisplayField>
    <errorMessage>Closing should be in 0 to 100.</errorMessage>
</ValidationRule>
