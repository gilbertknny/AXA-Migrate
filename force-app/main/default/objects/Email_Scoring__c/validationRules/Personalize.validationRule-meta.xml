<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Personalize</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Personalize__c &lt;0,Personalize__c &gt;100)</errorConditionFormula>
    <errorDisplayField>Personalize__c</errorDisplayField>
    <errorMessage>Personalize should be in 0 to 100.</errorMessage>
</ValidationRule>
