<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Responsiveness</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Responsiveness__c &lt;0,Responsiveness__c &gt;100)</errorConditionFormula>
    <errorDisplayField>Responsiveness__c</errorDisplayField>
    <errorMessage>Responsiveness should be in 0 to 100.</errorMessage>
</ValidationRule>
